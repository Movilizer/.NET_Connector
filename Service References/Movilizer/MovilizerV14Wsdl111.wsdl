        <xsd:attribute name="timestamp" type="xsd:dateTime" use="required" />
      </xsd:complexType>
      <xsd:element name="MovilizerMoveletDeleted" type="movilizer:MovilizerMoveletDeleted" />
      <xsd:complexType name="MovilizerMoveletDeleted">
        <xsd:attribute name="moveletKey" type="movilizer:nativeKey" use="required" />
        <xsd:attribute name="moveletKeyExtension" type="movilizer:nativeKey" use="optional" />
        <xsd:attribute name="moveletVersion" type="xsd:long" use="optional" />
        <xsd:attribute name="timestamp" type="xsd:dateTime" use="required" />
        <xsd:attribute name="manual" type="xsd:boolean" use="required" />
      </xsd:complexType>
      <xsd:element name="MovilizerMoveletAssignmentDeleted" type="movilizer:MovilizerMoveletAssignmentDeleted" />
      <xsd:complexType name="MovilizerMoveletAssignmentDeleted">
        <xsd:attribute name="moveletKey" type="movilizer:nativeKey" use="required" />
