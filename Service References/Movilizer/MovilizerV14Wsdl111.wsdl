        <xsd:attribute name="message" type="xsd:string" use="required" />
        <xsd:attribute name="requestTrackingKey" type="movilizer:nativeKey" use="optional" />
        <xsd:attribute name="timestamp" type="xsd:dateTime" use="required" />
        <xsd:attribute name="moveletKey" type="movilizer:nativeKey" use="optional" />
        <xsd:attribute name="moveletKeyExtension" type="movilizer:nativeKey" use="optional" />
        <xsd:attribute name="moveletName" type="movilizer:string255" use="optional" />
        <xsd:attribute name="participantKey" type="movilizer:nativeKey" use="optional" />
        <xsd:attribute name="deviceAddress" type="movilizer:deviceAddress" use="optional" />
        <xsd:attribute name="participantName" type="movilizer:string31" use="optional" />
        <xsd:attribute name="masterdataPool" type="movilizer:nativeKey" use="optional" />
        <xsd:attribute name="masterdataGroup" type="movilizer:clientKey" use="optional" />
        <xsd:attribute name="masterdataKey" type="movilizer:clientKey" use="optional" />
      </xsd:complexType>
      <xsd:element name="MovilizerMoveletTableColumnDefinition" type="movilizer:MovilizerMoveletTableColumnDefinition" />
      <xsd:complexType name="MovilizerMoveletTableColumnDefinition">
        <xsd:attribute name="columnID" type="movilizer:string255" use="required" />
