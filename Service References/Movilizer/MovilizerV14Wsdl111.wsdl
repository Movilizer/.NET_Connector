        <xsd:attribute name="deviceAddress" type="movilizer:deviceAddress" use="required" />
        <xsd:attribute name="timestamp" type="xsd:dateTime" use="required" />
        <xsd:attribute name="manual" type="xsd:boolean" use="required" />
