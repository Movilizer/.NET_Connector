      <xsd:complexType name="MovilizerMasterdataAck">
        <xsd:attribute name="pool" type="movilizer:nativeKey" use="required" />
        <xsd:attribute name="key" type="movilizer:clientKey" use="required" />
        <xsd:attribute name="group" type="movilizer:clientKey" use="required" />
        <xsd:attribute name="timestamp" type="xsd:dateTime" use="required" />
        <xsd:attribute name="masterdataAckKey" type="movilizer:nativeKey" use="optional" />
