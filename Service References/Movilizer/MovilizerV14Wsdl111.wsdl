      <xsd:element name="MovilizerMasterdataError" type="movilizer:MovilizerMasterdataError" />
      <xsd:complexType name="MovilizerMasterdataError">
        <xsd:attribute name="message" type="xsd:string" use="required" />
        <xsd:attribute name="validationErrorCode" type="xsd:string" use="optional" />
        <xsd:attribute name="pool" type="movilizer:nativeKey" use="required" />
        <xsd:attribute name="key" type="movilizer:clientKey" use="optional" />
        <xsd:attribute name="group" type="movilizer:clientKey" use="optional" />
        <xsd:attribute name="timestamp" type="xsd:dateTime" use="required" />
        <xsd:attribute name="masterdataAckKey" type="movilizer:nativeKey" use="optional" />
      </xsd:complexType>
      <xsd:element name="MovilizerMasterdataDeleted" type="movilizer:MovilizerMasterdataDeleted" />
      <xsd:complexType name="MovilizerMasterdataDeleted">
        <xsd:attribute name="pool" type="movilizer:nativeKey" use="required" />
        <xsd:attribute name="key" type="movilizer:clientKey" use="optional" />
        <xsd:attribute name="group" type="movilizer:clientKey" use="optional" />
        <xsd:attribute name="timestampMasterdata" type="xsd:dateTime" use="optional" />
        <xsd:attribute name="timestamp" type="xsd:dateTime" use="required" />
        <xsd:attribute name="masterdataAckKey" type="movilizer:nativeKey" use="optional" />
      </xsd:complexType>
      <xsd:element name="MovilizerDocumentAck" type="movilizer:MovilizerDocumentAck" />
      <xsd:complexType name="MovilizerDocumentAck">
        <xsd:attribute name="pool" type="movilizer:nativeKey" use="required" />
        <xsd:attribute name="key" type="movilizer:clientKey" use="required" />
        <xsd:attribute name="timestamp" type="xsd:dateTime" use="required" />
        <xsd:attribute name="documentAckKey" type="movilizer:nativeKey" use="optional" />
      </xsd:complexType>
      <xsd:element name="MovilizerDocumentError" type="movilizer:MovilizerDocumentError" />
      <xsd:complexType name="MovilizerDocumentError">
        <xsd:attribute name="message" type="xsd:string" use="required" />
        <xsd:attribute name="validationErrorCode" type="xsd:string" use="optional" />
        <xsd:attribute name="pool" type="movilizer:nativeKey" use="required" />
        <xsd:attribute name="key" type="movilizer:clientKey" use="optional" />
        <xsd:attribute name="timestamp" type="xsd:dateTime" use="required" />
        <xsd:attribute name="documentAckKey" type="movilizer:nativeKey" use="optional" />
      </xsd:complexType>
      <xsd:element name="MovilizerDocumentDeleted" type="movilizer:MovilizerDocumentDeleted" />
      <xsd:complexType name="MovilizerDocumentDeleted">
        <xsd:attribute name="pool" type="movilizer:nativeKey" use="required" />
