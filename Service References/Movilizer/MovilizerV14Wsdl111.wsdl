        <xsd:attribute default="false" name="receiveStatusMessages" type="xsd:boolean" use="optional" />
        <xsd:attribute name="smsText" type="movilizer:string255" use="optional" />
        <xsd:attribute name="mailText" type="movilizer:string1023" use="optional" />
        <xsd:attribute name="characterLimit" type="xsd:int" use="optional" />
      </xsd:complexType>
      <xsd:element name="MovilizerConfigurationContact" type="movilizer:MovilizerConfigurationContact" />
      <xsd:complexType name="MovilizerConfigurationContact">
        <xsd:annotation>
          <xsd:documentation>
				A contact maintained for the system.
			</xsd:documentation>
        </xsd:annotation>
        <xsd:attribute name="contactName" type="movilizer:string255" use="required" />
        <xsd:attribute default="required" name="email" type="movilizer:string255" />
        <xsd:attribute default="required" name="phone" type="movilizer:string255" />
        <xsd:attribute name="contactRole" type="movilizer:configContactRole" use="required" />
        <xsd:attribute name="street" type="movilizer:string255" use="optional" />
        <xsd:attribute name="postalcode" type="movilizer:string255" use="optional" />
        <xsd:attribute name="city" type="movilizer:string255" use="optional" />
        <xsd:attribute name="province" type="movilizer:string255" use="optional" />
        <xsd:attribute name="country" type="movilizer:string255" use="optional" />
      </xsd:complexType>
      <xsd:element name="MovilizerMoveletDelete" type="movilizer:MovilizerMoveletDelete" />
      <xsd:complexType name="MovilizerMoveletDelete">
        <xsd:annotation>
          <xsd:documentation>
				A MoveletDelete deletes an entire Movelet from the cloud or all Movelets matching a moveletKey when ignoring the extensionKey.
			</xsd:documentation>
        </xsd:annotation>
        <xsd:attribute name="moveletKey" type="movilizer:nativeKey" use="optional" />
        <xsd:attribute name="moveletKeyExtension" type="movilizer:nativeKey" use="optional" />
        <xsd:attribute default="false" name="ignoreExtensionKey" type="xsd:boolean" use="optional" />
      </xsd:complexType>
      <xsd:element name="MovilizerMoveletAssignmentDelete" type="movilizer:MovilizerMoveletAssignmentDelete" />
      <xsd:complexType name="MovilizerMoveletAssignmentDelete">
        <xsd:annotation>
          <xsd:documentation>
				A MoveletAssignmentDelete can delete assignments for a single participant or for a single movelet, depending on the combination
				of deviceAddress and moveletKey. At least one of both parameters must be set!
			</xsd:documentation>
        </xsd:annotation>
        <xsd:attribute name="deviceAddress" type="movilizer:deviceAddress" use="optional" />
        <xsd:attribute name="moveletKey" type="movilizer:nativeKey" use="optional" />
        <xsd:attribute default="true" name="hardDelete" type="xsd:boolean" use="optional" />
        <xsd:attribute name="moveletKeyExtension" type="movilizer:nativeKey" use="optional" />
        <xsd:attribute default="false" name="ignoreExtensionKey" type="xsd:boolean" use="optional" />
      </xsd:complexType>
      <xsd:element name="MovilizerUploadStatusMessage" type="movilizer:MovilizerUploadStatusMessage" />
      <xsd:complexType name="MovilizerUploadStatusMessage">
        <xsd:annotation>
          <xsd:documentation>
				Enables any backend to send status-messages to the MovilizerMonitoring.
			</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
