      </xsd:complexType>
      <xsd:element name="MovilizerMoveletTableCell" type="movilizer:MovilizerMoveletTableCell" />
      <xsd:complexType name="MovilizerMoveletTableCell">
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="1" name="value" type="movilizer:string1023" />
          <xsd:element minOccurs="0" maxOccurs="1" name="valueExpression" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>A dynamic MEL expression that allows a dynamic representation of the Movelet table cell value.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="columnID" type="movilizer:string255" use="required" />
