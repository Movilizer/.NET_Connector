        <xsd:attribute default="NORMAL" name="okTextFontSize" type="movilizer:fontSize" use="optional" />
        <xsd:attribute default="NORMAL" name="okTextFontStyle" type="movilizer:fontStyle" use="optional" />
      </xsd:complexType>
      <xsd:element name="MovilizerMetaAnswer" type="movilizer:MovilizerMetaAnswer" />
      <xsd:complexType name="MovilizerMetaAnswer">
        <xsd:attribute name="key" type="movilizer:nativeKey" use="required" />
        <xsd:attribute name="text" type="movilizer:string1023" use="required" />
        <xsd:attribute name="action" type="movilizer:actionType" use="optional" />
        <xsd:attribute name="metaAction" type="movilizer:metaActionType" use="required" />
        <xsd:attribute name="position" type="xsd:short" use="optional" />
      </xsd:complexType>
      <xsd:element name="MovilizerQuestion" type="movilizer:MovilizerQuestion" />
      <xsd:complexType name="MovilizerQuestion">
        <xsd:sequence>
          <xsd:element minOccurs="1" maxOccurs="unbounded" name="answer" type="movilizer:MovilizerAnswer" />
          <xsd:element minOccurs="0" maxOccurs="1" name="text" type="movilizer:string1023" />
          <xsd:element minOccurs="0" maxOccurs="64" name="useParameter" type="movilizer:nativeKey">
            <xsd:annotation>
              <xsd:documentation>Specify which parameters to use in the reply. Allows 8 parameters max.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" maxOccurs="1" name="defineParameter" type="movilizer:MovilizerParameterDescriptor">
            <xsd:annotation>
              <xsd:documentation>Initiates a cyclic question. The parameter that has to be filled with the selected answers value. Works only with SingleSelectQuestions(type=2).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="restriction" type="movilizer:MovilizerRestriction">
            <xsd:annotation>
              <xsd:documentation>Prevents the user from leaving the screen while the Restriction is not satisfied. Can also be used to perform operations using MEL.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="validation" type="movilizer:MovilizerValidation">
            <xsd:annotation>
              <xsd:documentation>Ensures that the user enters valid data on a screen. Validations allow either to be warnings or errors.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" maxOccurs="1" name="onEnterAssignment" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>
						The MEL expression which is executed when entering the Question.
					</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" maxOccurs="1" name="onLeaveOkPrepareAssignment" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>
						The MEL expression which is executed when leaving the Question by pressing OK. This OK expression is being executed at the beginning of the question-transition, before the transition can be cancelled via a Validation.
					</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" maxOccurs="1" name="onLeaveOkPersistAssignment" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>
						The MEL expression which is executed when leaving the Question by pressing OK. This OK expression is being executed at the end of the question-transition, after all Validations succeeded.
					</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" maxOccurs="1" name="onLeaveBackAssignment" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>
						The MEL expression which is executed when leaving the Question by pressing back.
					</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" maxOccurs="1" name="onScreenValueChangeEvent" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>
						MEL function that is called on value changes on the screen. Function signature must be: function(answerKey, clientkey, value) 
					</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" maxOccurs="1" name="onExternalEvent" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>
						MEL function that is called by external events. Function signature must be: function(evttype, data)
					</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" maxOccurs="1" name="encryptionIV" type="movilizer:nativeKey" />
          <xsd:element minOccurs="0" maxOccurs="1" name="search" type="movilizer:MovilizerQuestionSearch" />
          <xsd:element minOccurs="0" maxOccurs="1" name="complex" type="movilizer:MovilizerQuestionComplexDescriptor" />
        </xsd:sequence>
        <xsd:attribute name="key" type="movilizer:nativeKey" use="required" />
        <xsd:attribute name="title" type="movilizer:string255" use="optional" />
        <xsd:attribute name="type" type="xsd:byte" use="required" />
        <xsd:attribute default="0" name="timeout" type="xsd:short" use="optional">
          <xsd:annotation>
            <xsd:documentation>Timeout for this question (in seconds). A positive value will trigger an OK Event, a negative value a Back Event.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="true" name="backNavigationAllowed" type="xsd:boolean" use="optional" />
        <xsd:attribute default="false" name="sortAnswersByClientKey" type="xsd:boolean" use="optional" />
        <xsd:attribute default="-1" name="sortColumnIndex" type="xsd:byte" use="optional">
          <xsd:annotation>
            <xsd:documentation>When using table questions, this will specify the initial sorting of the rows, using the column default sorting. If not specified, rows will
				follow the initial sorting by position.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="-1" name="groupColumnIndex" type="xsd:byte" use="optional">
          <xsd:annotation>
            <xsd:documentation>
					When using table questions, this will specify the column which will be used for grouping rows. This is automatically indicate that the table is grouped and will respond to methods like isGroupMode=true, isGroupingRow(index). Grouping column will not be presented in table column UI structure
				</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="true" name="tableRowDetailsEnabled" type="xsd:boolean" use="optional" />
        <xsd:attribute default="true" name="tableOptionsEnabled" type="xsd:boolean" use="optional" />
        <xsd:attribute name="intermoveletSystemId" type="xsd:long" use="optional" />
        <xsd:attribute name="intermoveletMoveletKey" type="movilizer:nativeKey" use="optional" />
        <xsd:attribute default="" name="intermoveletMoveletKeyExtension" type="movilizer:nativeKey" use="optional" />
        <xsd:attribute default="0" name="intermoveletType" type="xsd:byte" use="optional" />
        <xsd:attribute name="icon" type="xsd:byte" use="optional" />
        <xsd:attribute default="LEFT" name="labelAlignment" type="movilizer:alignment" use="optional" />
        <xsd:attribute default="LEFT" name="valueAlignment" type="movilizer:alignment" use="optional" />
        <xsd:attribute default="NORMAL" name="labelFontSize" type="movilizer:fontSize" use="optional" />
        <xsd:attribute default="NORMAL" name="labelFontStyle" type="movilizer:fontStyle" use="optional" />
        <xsd:attribute default="NORMAL" name="valueFontSize" type="movilizer:fontSize" use="optional" />
        <xsd:attribute default="NORMAL" name="valueFontStyle" type="movilizer:fontStyle" use="optional" />
        <xsd:attribute default="LEFT" name="titleAlignment" type="movilizer:alignment" use="optional" />
        <xsd:attribute default="NORMAL" name="titleFontSize" type="movilizer:fontSize" use="optional" />
        <xsd:attribute default="NORMAL" name="titleFontStyle" type="movilizer:fontStyle" use="optional" />
        <xsd:attribute default="LEFT" name="questionTextAlignment" type="movilizer:alignment" use="optional" />
        <xsd:attribute default="LARGE" name="questionTextFontSize" type="movilizer:fontSize" use="optional" />
        <xsd:attribute default="BOLD" name="questionTextFontStyle" type="movilizer:fontStyle" use="optional" />
        <xsd:attribute default="NORMAL" name="backTextFontSize" type="movilizer:fontSize" use="optional" />
        <xsd:attribute default="NORMAL" name="backTextFontStyle" type="movilizer:fontStyle" use="optional" />
        <xsd:attribute default="NORMAL" name="okTextFontSize" type="movilizer:fontSize" use="optional" />
        <xsd:attribute default="NORMAL" name="okTextFontStyle" type="movilizer:fontStyle" use="optional" />
      </xsd:complexType>
      <xsd:complexType name="MovilizerQuestionSearch">
        <xsd:annotation>
          <xsd:documentation>
				Defines if the Question should enable the search feature and sets the mode of the search. 
			</xsd:documentation>
        </xsd:annotation>
        <xsd:attribute default="0" name="spacerMode" type="xsd:byte" use="optional" />
        <xsd:attribute default="false" name="caseSensitive" type="xsd:boolean" use="optional" />
        <xsd:attribute default="0" name="textCondition" type="xsd:byte" use="optional" />
      </xsd:complexType>
      <xsd:complexType name="MovilizerQuestionComplexDescriptor">
        <xsd:annotation>
          <xsd:documentation>
				Defines if the Question should be part of a ComplexUI Screen. 
			</xsd:documentation>
        </xsd:annotation>
        <xsd:attribute name="linearGroupId" type="movilizer:nativeKey" use="required" />
        <xsd:attribute name="gridGroupId" type="movilizer:nativeKey" use="optional" />
        <xsd:attribute name="groupTitle" type="movilizer:string255" use="optional" />
        <xsd:attribute default="false" name="linearInnerScrollbar" type="xsd:boolean" use="optional" />
        <xsd:attribute default="false" name="gridInnerScrollbar" type="xsd:boolean" use="optional" />
        <xsd:attribute default="false" name="gridHorizontalLayout" type="xsd:boolean" use="optional" />
        <xsd:attribute name="linearPos" type="xsd:byte" use="required" />
        <xsd:attribute name="gridPosX" type="xsd:byte" use="optional" />
        <xsd:attribute name="gridPosY" type="xsd:byte" use="optional" />
        <xsd:attribute default="1" name="gridWidth" type="xsd:byte" use="optional">
          <xsd:annotation>
            <xsd:documentation>The relative-width of this question on the screen. The value must be in the range of 1-15.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="1" name="gridHeight" type="xsd:byte" use="optional">
          <xsd:annotation>
            <xsd:documentation>The relative-height of this question on the screen. The value must be in the range of 1-15.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="1" name="linearHeight" type="xsd:byte" use="optional">
          <xsd:annotation>
            <xsd:documentation>The relative-height of this question on the screen. The value must be in the range of 1-15.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:element name="MovilizerParameterDescriptor" type="movilizer:MovilizerParameterDescriptor" />
      <xsd:complexType name="MovilizerParameterDescriptor">
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="1" name="paramExpression" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>
						The MEL expression which sets the parameter as a string that starts a cycle.
					</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="parameterKey" type="movilizer:nativeKey" use="required" />
        <xsd:attribute default="ANSWERID" name="parameterType" type="movilizer:parameterType" use="optional" />
      </xsd:complexType>
      <xsd:element name="MovilizerValidation" type="movilizer:MovilizerValidation" />
      <xsd:complexType name="MovilizerValidation">
        <xsd:annotation>
          <xsd:documentation />
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element minOccurs="1" maxOccurs="1" name="condition" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>
						The validation rule, which when evaluated has to result to true. A
						validation rule is a MEL expression that returns a boolean.
					</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="1" maxOccurs="1" name="text" type="movilizer:string1023" />
          <xsd:element minOccurs="0" maxOccurs="1" name="matchingAssignment" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>
						A MEL assignemnt which is executed if condition matches.
					</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="position" type="xsd:short" use="optional">
          <xsd:annotation>
            <xsd:documentation>The sequence number in which all restrictions will be checked.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="type" type="movilizer:validationType" use="required" />
        <xsd:attribute default="false" name="backwardNavigation" type="xsd:boolean" use="optional">
          <xsd:annotation>
            <xsd:documentation>
                    Specifies if the given validation is for back or forward navigation. When this is specified
                    and set to true - then the validation is considered for backward navigation. In any other
                    case - for forward navigation.
                </xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:element name="MovilizerRestriction" type="movilizer:MovilizerRestriction" />
      <xsd:complexType name="MovilizerRestriction">
        <xsd:annotation>
          <xsd:documentation>
				A restriction can be attached to a question to either check the user input
				or control the navigation flow. For checking user input, a restriction has 
				to be set as 'required' and an error message should be defined that can be
				shown to the user, while their input is not valid. The user will not be 
				able to continue to the next question until all required restrictions 
				evaluate to true. Further, for optional restrictions it is possible
				to set an alternative follow up question. In this case, all restrictions 
				will be checked ordered by their position and for the first one to evaluate
				to true, the follow up question will be used as the actual next question in
				the navigation flow.
			</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element minOccurs="1" maxOccurs="1" name="condition" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>
						The validation rule, which when evaluated has to result to true. A
						validation rule is a MEL expression that returns a boolean.
					</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" maxOccurs="1" name="matchingAssignment" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>
						A MEL assignemnt which is executed if condition matches.
					</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="position" type="xsd:short" use="optional">
          <xsd:annotation>
            <xsd:documentation>The sequence number in which all restrictions will be checked.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="nextQuestionKey" type="movilizer:nativeKey" use="required">
          <xsd:annotation>
            <xsd:documentation>
					The next question to which the navigation flow will jump, if the rule 
					evaluates to true. If no follow up is defined, the follow up of the 
					selected answer will be taken.
				</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:element name="MovilizerAnswer" type="movilizer:MovilizerAnswer" />
      <xsd:complexType name="MovilizerAnswer">
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="item" type="movilizer:MovilizerAnswerItem" />
          <xsd:element minOccurs="0" maxOccurs="1" name="text" type="movilizer:string1023" />
          <xsd:element minOccurs="0" maxOccurs="1" name="predefinedValue" type="movilizer:string1023" />
          <xsd:element minOccurs="0" maxOccurs="1" name="data" type="movilizer:binary" />
        </xsd:sequence>
        <xsd:attribute name="attributeType" type="xsd:byte" use="optional" />
        <xsd:attribute default="EQUAL" name="columnSizeType" type="movilizer:answerColumnSizeType" use="optional" />
        <xsd:attribute name="hotkey" type="movilizer:string15" use="optional" />
        <xsd:attribute name="key" type="movilizer:nativeKey" use="required" />
        <xsd:attribute name="clientKey" type="movilizer:clientKey" use="optional" />
        <xsd:attribute name="nextQuestionKey" type="movilizer:nativeKey" use="required">
          <xsd:annotation>
            <xsd:documentation>
					nextQuestionKey specifies the key of the followup question. To mark a screen as an end of movelet screen, simply specify the nextQuestionKey
					as "END" or "CANCEL".
				</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="position" type="xsd:short" use="optional">
          <xsd:annotation>
            <xsd:documentation>
					Position specifies the positioning of the answers within the question. If not specified, the answerKey is used for sorting. When using Table type
					of question, this specifies the row position and is mandatory for all answers. Row 0 is mandatory a dummy row, containing the column type
					definitions.
				</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="-1" name="groupId" type="xsd:byte" use="optional">
          <xsd:annotation>
            <xsd:documentation>
					When using Multi Text Screen questions, this is mandatory field for constraint type CONSTRAINT_GROUP. This attribute is used to group fields on the screen
					in expandable/collapsable sets.
				</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="-1" name="colIndex" type="xsd:byte" use="optional">
          <xsd:annotation>
            <xsd:documentation>
					When using table questions, this is mandatory field that specifies the column position in the grid.
				</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="-1" name="colWidth" type="xsd:byte" use="optional">
          <xsd:annotation>
            <xsd:documentation>
					Table Column width in characters 
				</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="false" name="colSortDesc" type="xsd:boolean" use="optional">
          <xsd:annotation>
            <xsd:documentation>
					When using table questions, this defines the intial sorting of the column (if not specified - uses Asc sorting). This is only
					considered in ordering the rows initially if sorting field is specified on Question level.
				</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="true" name="colSearchable" type="xsd:boolean" use="optional">
          <xsd:annotation>
            <xsd:documentation>
					When using table questions, this defines if the column that is defined by this dummy answer is searchable or not. The default value is true.
				</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="true" name="colSortable" type="xsd:boolean" use="optional">
          <xsd:annotation>
            <xsd:documentation>
					When using table questions, this defines if the column that is defined by this dummy answer is manually sortable or not. The default value is true.
				</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="colPriority" type="xsd:byte" use="optional">
          <xsd:annotation>
            <xsd:documentation>
					Column layout measurement priority
				</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="NONE" name="action" type="movilizer:actionType" use="optional" />
        <xsd:attribute default="-1" name="connectionTimeout" type="xsd:int" use="optional">
          <xsd:annotation>
            <xsd:documentation>Timeout that is applied only if the action is of type that triggers sync. 
				This timeout will replace the default connection timeout when executing sync. The default value is set to -1 which means that the default value will be taken</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="-1" name="readTimeout" type="xsd:int" use="optional">
          <xsd:annotation>
            <xsd:documentation>Timeout that is applied only if the action is of type that triggers sync. 
				This timeout will replace the default connection read timeout when executing sync. The default value is set to -1 which means that the default value will be taken</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="false" name="triggerScreenValueChangedEvent" type="xsd:boolean" use="optional">
          <xsd:annotation>
            <xsd:documentation>Specifies if the change expression should be executed when the value of this answer changes.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="false" name="timerBasedScreenValueChangedEvent" type="xsd:boolean" use="optional">
          <xsd:annotation>
            <xsd:documentation>Specifies if the change event should be triggered on focus change (true) or on timed event (e.g every 100ms after value is changed).</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="false" name="dummyAnswer" type="xsd:boolean" use="optional">
          <xsd:annotation>
            <xsd:documentation>Dummy answers are not visible on the client. Use a dummy answer if you want to use the MEL functions addAnswer or addAnswers. These functions replace the dummy answer with dynamically created new, visible answers</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="icon" type="xsd:byte" use="optional">
          <xsd:annotation>
            <xsd:documentation>Referenced icon ID. For standard icons positive values, for custom icons negative values. If no value is set, then the client will also look into the data field of the answer to find some image data there</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="false" name="sortAnswerItemsByClientKey" type="xsd:boolean" use="optional">
          <xsd:annotation>
            <xsd:documentation>If set to true then answer items will be sorted by client key - not by value.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="labelAlignment" type="movilizer:alignment" use="optional" />
        <xsd:attribute name="valueAlignment" type="movilizer:alignment" use="optional" />
        <xsd:attribute name="labelFontSize" type="movilizer:fontSize" use="optional" />
        <xsd:attribute name="labelFontStyle" type="movilizer:fontStyle" use="optional" />
        <xsd:attribute name="valueFontSize" type="movilizer:fontSize" use="optional" />
        <xsd:attribute name="valueFontStyle" type="movilizer:fontStyle" use="optional" />
      </xsd:complexType>
      <xsd:element name="MovilizerAnswerItem" type="movilizer:MovilizerAnswerItem" />
      <xsd:complexType name="MovilizerAnswerItem">
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="1" name="value" type="movilizer:string1023" />
        </xsd:sequence>
        <xsd:attribute name="clientKey" type="movilizer:clientKey" use="optional" />
        <xsd:attribute default="false" name="dummyAnswerItem" type="xsd:boolean" use="optional">
          <xsd:annotation>
            <xsd:documentation>Required for 1.8 clients and earlier. Obsolete when using 1.9+ clients.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:element name="MovilizerMoveletReset" type="movilizer:MovilizerMoveletReset" />
      <xsd:complexType name="MovilizerMoveletReset">
        <xsd:annotation>
          <xsd:documentation>
				A MoveletReset can reset a movelet for a single device, a movelet for all devices or all movelets for one device, depending on the combination
				of phoneNo and moveletKey. At least one of both parameters must be set!
			</xsd:documentation>
        </xsd:annotation>
        <xsd:attribute name="deviceAddress" type="movilizer:deviceAddress" use="optional" />
        <xsd:attribute name="moveletKey" type="movilizer:nativeKey" use="optional" />
        <xsd:attribute name="moveletKeyExtension" type="movilizer:nativeKey" use="optional" />
        <xsd:attribute default="false" name="ignoreExtensionKey" type="xsd:boolean" use="optional" />
      </xsd:complexType>
      <xsd:element name="MovilizerParticipantReset" type="movilizer:MovilizerParticipantReset" />
      <xsd:complexType name="MovilizerParticipantReset">
        <xsd:annotation>
          <xsd:documentation>
				A ParticipantReset resets the state of a mobile device in the Movilizer System. This will cause the deployment of the MovilizerClient
				Application to be triggered again. The state of the assigned movelets will not be changed.
			</xsd:documentation>
        </xsd:annotation>
        <xsd:attribute name="deviceAddress" type="movilizer:deviceAddress" use="required" />
      </xsd:complexType>
      <xsd:element name="MovilizerParticipantConfiguration" type="movilizer:MovilizerParticipantConfiguration" />
      <xsd:complexType name="MovilizerParticipantConfiguration">
        <xsd:annotation>
          <xsd:documentation>
				A ParticipantConfiguration is used to create a new participant or to update a participant which is already in the system
				Currently only the timeAllignment can be set!
			</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="attributeDelete" type="movilizer:string1023">
            <xsd:annotation>
              <xsd:documentation>Explicit delete of a previously defined attributed for an earlier version of this masterdata entry.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="attributeUpdate" type="movilizer:MovilizerAttributeEntry">
            <xsd:annotation>
              <xsd:documentation>Additional attributes that may be used within MAF. Previous attributes for earlier versions of the masterdata entry will be overwritten.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="groupsReset" type="xsd:long">
            <xsd:annotation>
              <xsd:documentation>A list of SystemIds for which the groups should be cleared</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="groupDelete" type="movilizer:MovilizerParticipantGroup">
            <xsd:annotation>
              <xsd:documentation>Removes the participant from a list of participant groups</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="groupAdd" type="movilizer:MovilizerParticipantGroup">
            <xsd:annotation>
              <xsd:documentation>Adds the participant to a list of participant groups</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="attributeFriendlySystem" type="xsd:long" use="optional" />
        <xsd:attribute name="deviceAddress" type="movilizer:deviceAddress" use="required" />
        <xsd:attribute name="name" type="movilizer:string31" use="optional" />
        <xsd:attribute name="isTimeAligned" type="xsd:boolean" use="optional" />
        <xsd:attribute default="false" name="resetSecret" type="xsd:boolean" use="optional" />
        <xsd:attribute default="false" name="resetGroups" type="xsd:boolean" use="optional" />
        <xsd:attribute default="-1" name="passwordHashType" type="xsd:int" use="optional">
          <xsd:annotation>
            <xsd:documentation>
					Specifies the password hash type for a participants password.
					-1 = Dont change password
					0 = Disable password
					1 = Set plaintext password
					2 = Set MD5 with 8 bytes of salt and 1000 iterations
					3 = Set SHA256 with 8 bytes of salt and 1000 iterations
					4 = Set SHA512 with 16 bytes of salt and 1000 iterations
				</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="passwordHashValue" type="xsd:string" use="optional">
          <xsd:annotation>
            <xsd:documentation>The hash value for the specified password hash type. See: PKCS#5 / rfc2898 / http://www.jasypt.org/api/jasypt/1.9.2/org/jasypt/digest/StandardStringDigester.html</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="isPrivateForSystemID" type="xsd:boolean" use="optional" />
        <xsd:attribute name="language" type="xsd:string" use="optional" />
        <xsd:attribute name="alternativeDeploymentAddress" type="movilizer:deviceAddress" use="optional" />
        <xsd:attribute name="validTillDate" type="xsd:dateTime" use="optional" />
        <xsd:attribute default="0" name="idleTTL" type="xsd:int" use="optional" />
