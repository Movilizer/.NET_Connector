        <xsd:sequence>
          <xsd:element minOccurs="1" maxOccurs="1" name="container" type="movilizer:MovilizerGenericUploadDataContainer" />
        </xsd:sequence>
        <xsd:attribute default="0" name="containerUploadPriority" type="xsd:short" use="optional">
          <xsd:annotation>
            <xsd:documentation>Priority that controls the order of Container-upload. Higher value means higher priority.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:element name="MovilizerResponse">
        <xsd:annotation>
          <xsd:documentation>
				The MovilizerResponse defines the return data for the request. Please note that the internal processing of the Movilizer platform works
				asynchronous. Therefore the Movilizer platform never returns a the MovilizerResponse for issues MovilizerRequest within the same request.
			</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="masterdataAck" type="movilizer:MovilizerMasterdataAck" />
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="masterdataError" type="movilizer:MovilizerMasterdataError" />
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="masterdataDeleted" type="movilizer:MovilizerMasterdataDeleted" />
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="documentAck" type="movilizer:MovilizerDocumentAck" />
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="documentError" type="movilizer:MovilizerDocumentError" />
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="documentDeleted" type="movilizer:MovilizerDocumentDeleted" />
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="moveletAck" type="movilizer:MovilizerMoveletAck" />
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="moveletError" type="movilizer:MovilizerMoveletError" />
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="moveletDeleted" type="movilizer:MovilizerMoveletDeleted" />
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="moveletAssignmentDeleted" type="movilizer:MovilizerMoveletAssignmentDeleted" />
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="moveletSynced" type="movilizer:MovilizerMoveletSynced" />
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="metaMoveletReply" type="movilizer:MovilizerMetaMoveletReply" />
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="replyMovelet" type="movilizer:MovilizerReplyMovelet" />
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="uploadContainer" type="movilizer:MovilizerUploadDataContainer" />
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="participantAck" type="movilizer:MovilizerParticipantAck" />
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="participantDeploymentSmsSent" type="movilizer:MovilizerParticipantDeploymentSmsSent" />
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="participantInstall" type="movilizer:MovilizerParticipantInstall" />
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="participantInstallAck" type="movilizer:MovilizerParticipantInstallAck" />
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="participantInstallError" type="movilizer:MovilizerParticipantInstallError" />
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="participantDeleted" type="movilizer:MovilizerParticipantDeleted" />
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="statusMessage" type="movilizer:MovilizerStatusMessage" />
          </xsd:sequence>
          <xsd:attribute name="systemId" type="xsd:long" use="required" />
          <xsd:attribute name="responseQueue" type="movilizer:string7" use="optional" />
          <xsd:attribute name="currentServerTime" type="xsd:dateTime" use="required" />
          <xsd:attribute name="numPendingResponseBuckets" type="xsd:int" use="optional" />
          <xsd:attribute name="requestAcknowledgeKey" type="movilizer:nativeKey" use="optional">
            <xsd:annotation>
              <xsd:documentation>This field is used by the backend to acknowledge the last request: The value given in this field must be given along with the next request.</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:complexType>
      </xsd:element>
