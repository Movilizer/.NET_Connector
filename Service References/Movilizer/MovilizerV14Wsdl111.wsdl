        <xsd:attribute name="moveletKey" type="movilizer:nativeKey" use="required" />
        <xsd:attribute name="moveletKeyExtension" type="movilizer:nativeKey" use="optional" />
        <xsd:attribute name="moveletVersion" type="xsd:long" use="optional" />
        <xsd:attribute name="timestamp" type="xsd:dateTime" use="required" />
      </xsd:complexType>
      <xsd:element name="MovilizerMoveletError" type="movilizer:MovilizerMoveletError" />
      <xsd:complexType name="MovilizerMoveletError">
        <xsd:attribute name="message" type="xsd:string" use="required" />
        <xsd:attribute name="validationErrorCode" type="xsd:string" use="optional" />
        <xsd:attribute name="moveletKey" type="movilizer:nativeKey" use="required" />
